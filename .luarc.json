{
  "completion.callSnippet": "Replace",
  "completion.keywordSnippet": "Replace",
  "completion.showWord": "Fallback",
  "runtime": {
    "version": "Lua 5.1"
  },
  "telemetry.enable": false,
  "workspace.ignoreSubmodules": false,
  "workspace.ignoreDir": [
    ".internal",
    ".defold"
  ],
  "workspace.library": [
    ".internal",
    ".defold"
  ],
  "diagnostics.disable": [
    "lowercase-global",
    "missing-parameter",
    "redefined-local"
  ]
}
